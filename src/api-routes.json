{
  "/v1/problems": {
    "get": {
      "ctrl": "ProblemController",
      "method": "searchProblems",
      "public": true
    }
  },
  "/v1/problems/:id": {
    "get": {
      "ctrl": "ProblemController",
      "method": "getProblem",
      "public": true
    }
  },
  "/v1/login": {
    "post": {
      "ctrl": "AuthController",
      "method": "login",
      "public": true
    }
  },
  "/v1/logout": {
    "post": {
      "ctrl": "AuthController",
      "method": "logout",
      "public": true
    }
  },
  "/v1/verify-email/:code": {
    "post": {
      "ctrl": "AuthController",
      "method": "verifyEmail",
      "public": true
    }
  },
  "/v1/register": {
    "post": {
      "ctrl": "AuthController",
      "method": "register",
      "public": true
    }
  },
  "/v1/code-templates/:language": {
    "get": {
      "ctrl": "CodeTemplateController",
      "method": "getCodeTemplate",
      "public": true
    }
  },
  "/v1/problems/:id/submit": {
    "post": {
      "ctrl": "SubmissionController",
      "method": "submit"
    }
  },
  "/v1/submissions/recent": {
    "get": {
      "ctrl": "SubmissionController",
      "method": "getRecentSubmissions",
      "public": true
    }
  },
  "/v1/submissions/:notifyKey/progress": {
    "post": {
      "ctrl": "SubmissionController",
      "method": "notifyProgress",
      "public": true
    }
  },
  "/v1/submissions/:notifyKey/result": {
    "post": {
      "ctrl": "SubmissionController",
      "method": "submitTestResult",
      "public": true
    }
  },
  "/v1/ranking": {
    "get": {
      "ctrl": "RankingController",
      "method": "getRanking",
      "public": true
    }
  },
  "/v1/ranking/filter": {
    "get": {
      "ctrl": "RankingController",
      "method": "getRankingFilter",
      "public": true
    }
  },
  "/v1/user/:username": {
    "get": {
      "ctrl": "UserController",
      "method": "getUser",
      "public": true
    }
  }
}
